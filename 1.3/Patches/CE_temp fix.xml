<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Combat Extended</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="Ammo_66mmExplosiveBolt_HE-I"]</xpath>
					<nomatch Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationRemove">
								<xpath>
									Defs/RecipeDef[defName="RecycleMechExplosives"]/ingredients/li/filter/thingDefs/li[text()='Ammo_66mmExplosiveBolt_HE-I'] |
									Defs/RecipeDef[defName="RecycleMechExplosives"]/fixedIngredientFilter/thingDefs/li[text()='Ammo_66mmExplosiveBolt_HE-I']
								</xpath>
							</li>
						</operations>
					</nomatch>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
