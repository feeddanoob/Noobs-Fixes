<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Fallout Traits</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>
						Defs/TraitDef[
							defName="FT_ActionBoy" or 
							defName="FT_ActionGirl"]/modExtensions
					</xpath>
					<match Class="PatchOperationReplace">
						<xpath>
							Defs/TraitDef[
								defName="FT_ActionBoy" or 
								defName="FT_ActionGirl"]/modExtensions
						</xpath>
						<value>
							<modExtensions>
								<li Class="SyrTraitValue.TraitValueExtension">
									<traitValues>
										<li>30</li>
									</traitValues>
								</li>
							</modExtensions>
						</value>
					</match>
					<nomatch Class="PatchOperationAddModExtension">
						<xpath>
							Defs/TraitDef[
								defName="FT_ActionBoy" or 
								defName="FT_ActionGirl"]
						</xpath>
						<value>
							<li Class="SyrTraitValue.TraitValueExtension">
								<traitValues>
									<li>30</li>
								</traitValues>
							</li>
						</value>
					</nomatch>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>Defs/TraitDef[defName="FT_Philanthropist"]/modExtensions</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/TraitDef[defName="FT_Philanthropist"]/modExtensions</xpath>
						<value>
							<modExtensions>
								<li Class="SyrTraitValue.TraitValueExtension">
									<traitValues>
										<li>1, 20</li>
									</traitValues>
								</li>
							</modExtensions>
						</value>
					</match>
					<nomatch Class="PatchOperationAddModExtension">
						<xpath>Defs/TraitDef[defName="FT_Philanthropist"]</xpath>
						<value>
							<li Class="SyrTraitValue.TraitValueExtension">
								<traitValues>
									<li>1, 20</li>
								</traitValues>
							</li>
						</value>
					</nomatch>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>Defs/TraitDef[defName="FT_Bargaining"]/modExtensions</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/TraitDef[defName="FT_Bargaining"]/modExtensions</xpath>
						<value>
							<modExtensions>
								<li Class="SyrTraitValue.TraitValueExtension">
									<traitValues>
										<li>1, 10</li>
										<li>2, 15</li>
									</traitValues>
								</li>
							</modExtensions>
						</value>
					</match>
					<nomatch Class="PatchOperationAddModExtension">
						<xpath>Defs/TraitDef[defName="FT_Bargaining"]</xpath>
						<value>
							<li Class="SyrTraitValue.TraitValueExtension">
								<traitValues>
									<li>1, 10</li>
									<li>2, 15</li>
								</traitValues>
							</li>
						</value>
					</nomatch>
				</li>
			</operations>
		</match>
	<Operation>
</Patch>