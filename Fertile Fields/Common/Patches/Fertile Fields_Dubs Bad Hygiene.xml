<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- Remove Ash -->
				<li Class="PatchOperationRemove">
					<xpath>
						Defs/RecipeDef[
							defName = "Make_Ash2" or
							defName = "Make_Ash12"] |
						Defs/ThingDef[defName="D9Ash"]
					</xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
