<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VCE_Grass"]</xpath>
					<value>
						<li Class="SeedsPleaseLite.Seedless"/>
					</value>
				</li>
				<li Class="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VCE_TallGrass"]</xpath>
					<value>
						<li Class="SeedsPleaseLite.Seedless"/>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
