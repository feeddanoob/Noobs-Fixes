		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="DankPyon_Hampshire" or defName="DankPyon_Gloucestershire" or defName="DankPyon_NorthernBoar"]/race</xpath>
			<value>
				<specialTrainables>
					<li MayRequire="Ludeon.RimWorld.Odyssey">Forage</li>
				</specialTrainables>
			</value>
		</nomatch>
	</Operation>
</Patch>
