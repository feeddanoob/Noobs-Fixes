				<soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding_Metal</soundMeleeHitBuilding>
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName="VFEP_Mech_Spidermine"]/race[soundMeleeHitBuilding="Pawn_Melee_Punch_HitBuilding"]</xpath>
