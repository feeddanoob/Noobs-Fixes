			<xpath>Defs/ThingDef[defName="VFEP_Mech_Spidermine"]/race/soundMeleeHitBuilding</xpath>
			<value>
				<soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding_Metal</soundMeleeHitBuilding>
			</value>
		</match>
	</Operation>
</Patch>
