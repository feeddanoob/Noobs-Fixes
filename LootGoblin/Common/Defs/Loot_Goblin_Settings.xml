		<defName>LG_Rule_feeddanoobEdit</defName>
		<rules>
			<!-- What the hack?! - A little expansion -->
			<li>
				<match>
					<category>^WTH_.*Modules$</category>
				</match>
				<lootType>LG_Type_WTHModules</lootType>
			</li>
			<li>
				<match>
					<name>^WTH_.*Module$</name>
				</match>
				<lootType>LG_Type_WTHModules</lootType>
			</li>
			<!-- Dubs Rimatomics -->
			<li>
				<match>
					<category>NuclearFuel</category>
				</match>
