<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Begin Subgroups -->
	
	<!-- What the Hack?! subgroup -->
	<LootGoblin.LootTypeDef>
		<defName>LG_Type_WTHModules</defName>
		<label>What the Hack - Modules</label>
		<parent>LG_Type_Manufactured</parent>
	</LootGoblin.LootTypeDef>
	<!-- Dubs Rimatomics subgroup -->
	<LootGoblin.LootTypeDef>
		<defName>LG_Type_DubsRimatomics</defName>
