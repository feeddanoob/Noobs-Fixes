			<!-- Project Rimfactory Revived -->
			<li>
				<match>
					<name>PRF_DataDisk</name>
				</match>
				<lootType>LG_Type_Ignore</lootType>
			</li>
			<!-- Giddy-Up! Mechanoids -->
			<li>
				<match>
					<name>GU_Mech_GiddyUpModule</name>
				</match>
				<lootType>LG_Type_WTHModules</lootType>
			</li>
			<!-- Ideology (for relics) -->
